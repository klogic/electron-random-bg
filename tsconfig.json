{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "outDir": "./out"
  },
  "include": ["*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
